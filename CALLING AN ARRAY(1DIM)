#include<stdio.h>
void addarray(int a1[],int a2[]);
void addarray(int a1[],int a2[])
{
    int res[50];
    for(int i=0;i<5;i++)
    {
        res[i]=a1[i]+a2[i];
        printf("%d",res[i]);
    }
}
int main()
{
    int array1[5]={1,2,3,4,5};
    int array2[5]={4,3,2,1,5};
    addarray(array1,array2);
    return 0;
}
