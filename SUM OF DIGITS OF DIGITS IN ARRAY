#include<stdio.h>
#include<stdio.h>
int sumdigit(int n);
int sumdigit(int n)
{
    int m=0;
    for(int i=0;n>0;i++)
    {
        m=m+n%10;
        n=n/10;
    }
    return m;
}
int main()
{
    int sum=0,a[5]={10,23,10,34,4};
    for(int i=0;i<5;i++)
    {
        sum=sum+sumdigit(a[i]);
    }
    printf("the sum of digits is %d",sum);
    return 0;
}
